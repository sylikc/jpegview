        BEGIN
		  MENUITEM "_empty_",					IDM_ABOUT
        END
	  MENUITEM SEPARATOR
	  MENUITEM "About JPEGView...",				IDM_ABOUT
	  MENUITEM SEPARATOR
	  MENUITEM "Exit\t0xffff",					IDM_EXIT
    END 
 
